{
  "env": {
    "browser": true，
    "serviceworker": true
  },
  "extends": [
    "eslint:recommended"，
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"，
    "sourceType": "module"
  },

  "rules": {
    "no-unused-vars": "warn",
    "prettier/prettier": "warn" 
  }
}
